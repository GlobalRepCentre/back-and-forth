{
  "name": "back-and-forth",
  "version": "0.0.1",
  "description": "Back & Forth is an open source platform that lets you create interactive, team-based experiences wherever you want them.",
  "main": "backend/backend-server.ts",
  "scripts": {
    "pretest": "NODE_ENV=test ./node_modules/.bin/ts-node backend/db/reset-db.ts",
    "test": "NODE_ENV=test jest",
    "migrate": "ts-node backend/db/migrate.ts",
    "server": "node -r ts-node/register --nolazy --inspect=3334 backend/backend-server.ts",
    "build": "cd frontend && webpack --mode production",
    "watch": "cd frontend && webpack --watch --mode development",
    "lint": "tslint -p .",
    "interactive-multisession": "./node_modules/.bin/ts-node integration/interactive-multisession.ts"
  },
  "author": "From the Storm",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "~1.19.0",
    "connect-redis": "~3.3.3",
    "express": "^4.17.1",
    "express-session": "~1.17.1",
    "express-ws": "~4.0.0",
    "js-interpreter": "^1.4.6",
    "js-yaml": "^3.14.0",
    "json-rpc-peer": "~0.16.0",
    "massive": "~4.7.2",
    "nodemailer": "~4.6.4",
